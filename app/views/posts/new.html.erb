<%= form_for Post.new, html: {multipart: true}  do |f| %>
	
	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :image %>
	<%= f.file_field :image %>

	<%= f.label :tag_names, 'Tags' %>
	<%= f.text_field :tag_names, autocomplete: :off, data:{role:'tagsinput'}%>

	<%= f.label :address, 'Address' %>
	<%= f.text_field :address %>

	<%= f.button 'Create Post' %>

<% end %>		
